<h1>Room visits stats</h1>

<table class="table">
  <tr>
    <th>Room Id</th>
    <th>User Id</th>
    <th>Email</th>
    <th>Time Spent (Sec.)</th>
  </tr>
  <% @stats.each do |s| %>
      <tr>
        <td><%= s.id %></td>
        <td><%= s.user_id %></td>
        <td><%= s.email %></td>
        <td><%= s.time_spent/1000 %></td>
      </tr>
  <% end %>
</table>