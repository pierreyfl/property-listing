<div class="row">
  <div class="col-md-3">
    <%= render 'agency_setting_sidebar' %>
  </div>
  <div class="col-md-9">
    <div class="panel panel-default">
      <div class="panel-heading">Agencie</div>
      <div class="panel-body">
        <div class="container">
          <%= form_for :agency, url: agencies_path do |f| %>
            <div class="form-group">
              <label>Agent Admin Email</label>
              <%= f.text_field :email, class: 'form-control', placeholder: "email" %>
            </div>
            <div class="form-group">
              <label>Name</label>
              <%= f.text_field :name, class: "form-control", placeholder: "name" %>
            </div>
            <%= f.submit :submit, class: "btn btn-normal" %>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>
